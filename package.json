{
  "name": "freya-slides",
  "version": "0.30.0",
  "description": "Opinionated JSX based slides generator.",
  "homepage": "https://sw.cowtech.it/freya-slides",
  "repository": "github:ShogunPanda/freya-slides",
  "keywords": [
    "slides"
  ],
  "bugs": {
    "url": "https://github.com/ShogunPanda/freya-slides/issues"
  },
  "author": "Shogun <shogun@cowtech.it>",
  "license": "ISC",
  "private": false,
  "bin": {
    "freya": "dist/cli/index.js",
    "create-freya-slideset": "dist/cli/create.js"
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "LICENSE.md",
    "README.md"
  ],
  "type": "module",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "predev": "npm run prebuild",
    "dev": "swc -s -w -d dist src",
    "prebuild": "rm -rf dist && mkdir dist && cp -a src/assets dist",
    "build": "swc -d dist src",
    "postbuild": "concurrently npm:lint npm:typecheck",
    "format": "prettier -w src",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx src",
    "typecheck": "tsc -p . --emitDeclarationOnly",
    "ci": "npm run lint",
    "prepublishOnly": "npm run ci",
    "postpublish": "git push origin && git push origin -f --tags"
  },
  "dependencies": {
    "@fastify/formbody": "^7.4.0",
    "@fastify/static": "^6.11.2",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.94",
    "@unocss/core": "^0.57.0",
    "@unocss/preset-mini": "^0.57.0",
    "@unocss/transformer-directives": "^0.57.0",
    "chokidar": "^3.5.3",
    "commander": "^11.1.0",
    "fastify": "^4.24.3",
    "fastify-http-errors-enhanced": "^4.3.0",
    "glob": "^10.3.10",
    "js-yaml": "^4.1.0",
    "magic-string": "^0.30.5",
    "markdown-it": "^13.0.2",
    "pdfjs": "^2.5.2",
    "pino": "^8.16.1",
    "pino-pretty": "^10.2.3",
    "playwright": "^1.39.0",
    "playwright-chromium": "^1.39.0",
    "postcss": "^8.4.31",
    "postcss-discard-comments": "^6.0.0",
    "postcss-import": "^15.1.0",
    "postcss-minify-selectors": "^6.0.0",
    "postcss-nested": "^6.0.1",
    "postcss-normalize-whitespace": "^6.0.0",
    "pusher-js": "^8.3.0",
    "qrcode-generator": "^1.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rehype": "^13.0.1",
    "shiki": "^0.14.5",
    "unist-util-visit": "^5.0.0",
    "wait-on": "^7.0.1",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@cowtech/eslint-config": "^8.10.0",
    "@types/glob": "^8.1.0",
    "@types/js-yaml": "^4.0.8",
    "@types/markdown-it": "^13.0.5",
    "@types/node": "^20.8.7",
    "@types/postcss-import": "^14.0.2",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "@types/wait-on": "^5.3.3",
    "concurrently": "^8.2.2",
    "eslint": "^8.52.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  }
}
